CREATE TABLE `tictecfr_finding`.`mNSL1` ( `ID` INT NOT NULL AUTO_INCREMENT , `myID` INT NOT NULL , `name` VARCHAR(255) NOT NULL , `Ref_Integral` VARCHAR(255) NOT NULL , `SN` VARCHAR(255) NOT NULL , PRIMARY KEY (`ID`)) ENGINE = MyISAM; 

CREATE TABLE `tictecfr_finding`.`mNSL2` ( `ID` INT NOT NULL AUTO_INCREMENT , `myID` INT NOT NULL , `name` VARCHAR(255) NOT NULL , `CodeGUIP` MEDIUMTEXT NOT NULL , PRIMARY KEY (`ID`)) ENGINE = MyISAM; 

CREATE TABLE `tictecfr_finding`.`mNSL3` ( `ID` INT NOT NULL AUTO_INCREMENT , `myID` INT NOT NULL , `name` VARCHAR(255) NOT NULL , `Lib` VARCHAR(255) NOT NULL , `UseCase` VARCHAR(255) NOT NULL , PRIMARY KEY (`ID`)) ENGINE = MyISAM; 

CREATE TABLE `tictecfr_finding`.`mNSL4` ( `ID` INT NOT NULL AUTO_INCREMENT , `myID` INT NOT NULL , `name` VARCHAR(255) NOT NULL , `Identifier` INT NOT NULL , `Ref_Internal` VARCHAR(255) NOT NULL , `Ratio_Qty` VARCHAR(255) NOT NULL , PRIMARY KEY (`ID`)) ENGINE = MyISAM; 

CREATE TABLE `tictecfr_finding`.`mNSL5` ( `ID` INT NOT NULL AUTO_INCREMENT , `myID` INT NOT NULL , `name` VARCHAR(255) NOT NULL , `Description` MEDIUMTEXT NOT NULL , PRIMARY KEY (`ID`)) ENGINE = MyISAM; 

CREATE TABLE `tictecfr_finding`.`mNSL6` ( `ID` INT NOT NULL AUTO_INCREMENT , `myID` INT NOT NULL , `name` VARCHAR(255) NOT NULL , `Lieu` VARCHAR(255) NOT NULL , `Ratio_Qty` VARCHAR(255) NOT NULL , `Contact` MEDIUMTEXT NOT NULL , PRIMARY KEY (`ID`)) ENGINE = MyISAM; 

CREATE TABLE `tictecfr_finding`.`mNSL7` ( `ID` INT NOT NULL AUTO_INCREMENT , `myID` INT NOT NULL , `name` VARCHAR(255) NOT NULL , `Usages` VARCHAR(255) NOT NULL , `Ratio_Qty` VARCHAR(255) NOT NULL , PRIMARY KEY (`ID`)) ENGINE = MyISAM; 

CREATE TABLE `tictecfr_finding`.`mNSL8` ( `ID` INT NOT NULL AUTO_INCREMENT , `myID` INT NOT NULL , `name` VARCHAR(255) NOT NULL , `PN` VARCHAR(255) NOT NULL , PRIMARY KEY (`ID`)) ENGINE = MyISAM; 

CREATE TABLE `tictecfr_finding`.`Relations` ( `ID` INT NOT NULL AUTO_INCREMENT , `libSrc` VARCHAR(20) NOT NULL , `myIDSrc` INT NOT NULL , `LibDst` VARCHAR(20) NOT NULL , `myIDDst` INT NOT NULL , `ExtSrc` VARCHAR(2) NOT NULL , `Rel` VARCHAR(255) NOT NULL , `ExtDst` VARCHAR(2) NOT NULL , PRIMARY KEY (`ID`)) ENGINE = MyISAM; 
